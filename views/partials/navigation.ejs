<nav class="text-center" aria-label="Page navigation">
    <ul class="pagination">
        <% if(prevBtn){ %>
        <li class="page-item">
            <a class="page-link" href="/fetch/<%=parseInt(pageNumber)-1%>" aria-label="Previous">
                <span aria-hidden="true">&laquo;</span>
                <span class="sr-only">Previous</span>
            </a>
        </li>
        <% } %>
        <% if(nextBtn){ %>
        <li class="page-item">
            <a class="page-link" href="/fetch/<%=parseInt(pageNumber)+1%>" aria-label="Next">
                <span aria-hidden="true">&raquo;</span>
                <span class="sr-only">Next</span>
            </a>
        </li>
        <% } %>
    </ul>
</nav>